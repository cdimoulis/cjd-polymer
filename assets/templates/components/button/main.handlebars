{{#if display.raised}}
<paper-button raised class="{{display.button_color}} {{display.text_color}} {{display.extra_classes}}" {{{display.extra_attrs}}}>
{{else}}
<paper-button class="{{display.extra_classes}}" {{{display.extra_attrs}}}>
{{/if}}
  {{#if display.icon}}
  <iron-icon icon="{{display.icon}}">
  </iron-icon>
  {{/if}}
  {{display.text}}
</paper-button>
